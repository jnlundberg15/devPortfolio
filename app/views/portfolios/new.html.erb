<h1 Create a new porfolio item here </h1>
<% #new will only render, create actually saves to database %>

  <%= form_with(model: @portfolio_item, local: true) do |form| %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>


  <div class="field">
    <%= form.label :subtitle %>
    <%= form.text_field :subtitle %>
  </div>
  

  <div class="field">
    <%= form.label :body %>
    <%= form.text_area :body %>
  </div>

    <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
